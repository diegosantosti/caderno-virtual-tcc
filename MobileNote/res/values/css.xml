<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android">
    
    <style name="button_circle_main">
        <item name="android:padding">@dimen/margin_button_circle</item>
    </style>
    
    <style name="text_titulo_main">
       	<item name="android:textColor">@color/text_titulo_main</item>
       	<item name="android:gravity">center</item>
       	<item name="android:textStyle">bold</item>
       	<item name="android:textSize">25sp</item>
    </style>
    
    <style name="edit_pesquisa">
        <item name="android:textSize">18sp</item>
        <item name="android:padding">@dimen/padding_edit_pesq</item>
        <item name="android:background">@drawable/edit_text_pesquisa</item>
        <item name="android:textColor">@color/font_edit_main</item>
        <!--  <item name="android:focusable">false</item>-->
    </style>
    
    <style name="edit_text_pop_up">
        <item name="android:textSize">20sp</item>
        <item name="android:padding">@dimen/padding_edit_pesq</item>
        <item name="android:background">@drawable/edit_text</item>
        <item name="android:textColor">@color/font_edit</item>
        <!--  <item name="android:focusable">false</item>-->
    </style>
    
    <style name="text_pop_up">
       	<item name="android:textColor">@color/font_text_pop_up</item>
       	<item name="android:textStyle">bold</item>
       	<item name="android:textSize">18sp</item>
       	<item name="android:gravity">center</item>
    </style>
    
</resources>
